    (1,100) (2,100) (3,100) (4,100) (5,100) (6,100) (7,100) (8,100) (9,100) (10,100)
};
\addlegendentry{Gemini - Simple}

% Anthropic - Simple (Dash-dot red line)
\addplot[color=red, mark=diamond*, dashdotted, thick] coordinates {
    (1,100) (2,100) (3,100) (4,100) (5,100) (6,100) (7,100) (8,100) (9,100) (10,100)
};
\addlegendentry{Anthropic - Simple}

% ===== COMPLEX QUERIES (GREEN) =====

% Prolog - Complex (Solid green line)
\addplot[color=green!60!black, mark=*, solid, thick] coordinates {
    (1,100) (2,100) (3,100) (4,100) (5,100) (6,100) (7,100) (8,100) (9,100) (10,100)
};
\addlegendentry{Prolog - Complex}

% ChatGPT - Complex (Dotted green line)
\addplot[color=green!60!black, mark=square*, dotted, thick] coordinates {
    (1,100) (2,80) (3,80) (4,100) (5,80) (6,100) (7,100) (8,80) (9,100) (10,60)
};
\addlegendentry{ChatGPT - Complex}

% Gemini - Complex (Dashed green line)
\addplot[color=green!60!black, mark=triangle*, dashed, thick] coordinates {
    (1,80) (2,100) (3,100) (4,100) (5,80) (6,100) (7,100) (8,100) (9,100) (10,100)
};
\addlegendentry{Gemini - Complex}

% Anthropic - Complex (Dash-dot green line)
\addplot[color=green!60!black, mark=diamond*, dashdotted, thick] coordinates {
    (1,100) (2,100) (3,100) (4,80) (5,100) (6,100) (7,100) (8,100) (9,100) (10,100)
};
\addlegendentry{Anthropic - Complex}

% ===== MULTI-STEP QUERIES (PURPLE) =====

% Prolog - Multi-step (Solid blue line)
\addplot[color=blue, mark=*, solid, thick] coordinates {
    (1,100) (2,100) (3,100) (4,100) (5,100) (6,100) (7,100) (8,100) (9,100) (10,100)
};
\addlegendentry{Prolog - Multi-step}

% ChatGPT - Multi-step (Dotted blue line)
\addplot[color=blue, mark=square*, dotted, thick] coordinates {
    (1,80) (2,80) (3,80) (4,60) (5,60) (6,100) (7,80) (8,100) (9,80) (10,60)
};
\addlegendentry{ChatGPT - Multi-step}

% Gemini - Multi-step (Dashed blue line)
\addplot[color=blue, mark=triangle*, dashed, thick] coordinates {
    (1,80) (2,80) (3,80) (4,100) (5,80) (6,100) (7,80) (8,80) (9,100) (10,100)
};
\addlegendentry{Gemini - Multi-step}

% Anthropic - Multi-step (Dash-dot blue line)
\addplot[color=blue, mark=diamond*, dashdotted, thick] coordinates {
    (1,80) (2,100) (3,80) (4,100) (5,100) (6,100) (7,80) (8,80) (9,80) (10,80)
};
\addlegendentry{Anthropic - Multi-step}



\end{axis}
\end{tikzpicture}
\caption{Accuracy comparison across 10 experimental runs for 15 academic relationship queries. Each query set contains 5 simple (direct fact lookups), 5 complex (single rule applications), and 5 multi-step reasoning queries. The symbolic Prolog pipeline demonstrates consistent performance across all complexity levels.}
\label{fig:academics_15}
\end{figure}

