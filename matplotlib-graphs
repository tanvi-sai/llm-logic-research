}

# Function to create a single graph
def create_graph(data_dict, title, ylabel, max_y, filename):
    fig, ax = plt.subplots(figsize=(10, 6))
    x = np.arange(1, 11)
    
    # Colors for query types
    colors = {'simple': 'red', 'complex': 'green', 'multi-step': 'blue'}
    
    # Line styles for LLMs
    line_styles = {
        'prolog': '-',
        'chatgpt': ':',
        'gemini': '--',
        'anthropic': '-.'
    }
    
    # Markers for LLMs
    markers = {
        'prolog': 'o',
        'chatgpt': 's',
        'gemini': '^',
        'anthropic': 'D'
    }
    
    # Plot all lines
    for llm in ['prolog', 'chatgpt', 'gemini', 'anthropic']:
        for query_type in ['simple', 'complex', 'multi-step']:
            label = f"{llm.capitalize()} - {query_type.capitalize()}"
            ax.plot(x, data_dict[llm][query_type], 
                   color=colors[query_type], 
                   linestyle=line_styles[llm], 
                   marker=markers[llm], 
                   markersize=6, 
                   linewidth=2, 
                   label=label)
    
    ax.set_xlabel('Experiment Number', fontsize=12)
    ax.set_ylabel(ylabel, fontsize=12)
    ax.set_title(title, fontsize=14, fontweight='bold')
    ax.set_xticks(x)
    ax.set_ylim(0, 105)
    ax.grid(True, alpha=0.3)
    ax.legend(loc='center left', bbox_to_anchor=(1, 0.5), fontsize=8, ncol=1)
    
    plt.tight_layout()
    plt.savefig(filename, dpi=300, bbox_inches='tight')
    plt.close()
    print(f"Saved: {filename}")

# Generate all 6 graphs

# Academics 15
academics_15_data = {
    'prolog': academics_15_prolog,
    'chatgpt': academics_15_chatgpt,
    'gemini': academics_15_gemini,
    'anthropic': academics_15_anthropic
}
create_graph(academics_15_data, 
            'Academic Queries (15 Total: 5 Simple, 5 Complex, 5 Multi-step)', 
            'Accuracy (out of 5)', 
            105, 
            'academics_15_queries.png')

